 #include <stdio.h>
#include<string.h>
int main()
{
    long int t;
    int i,n,count=0;
    char s[10000];
    scanf("%d\n",&t);
    while(t)
    {
        int a=0;
       scanf("%d",&n);
       printf("\n");
       scanf("%s",s);
       
    for(i=0;i<=n;i++)
    {
        if((s[i]>=48)&&(s[i]<=57)){
             count++;
             
        }
    else {
        if(count!=0)
        a++;
        count=0;
    }
    }
    printf("%d\n",a);
    t--;
    }
    return 0;
}
